import java.util.*;
class Day
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
String a=sc.next();
String b=sc.next();
if(a.equals("Saturday")||a.equals("Sunday"))
{
System.out.print("yes");
}
else
{
System.out.print("no");
}
if(b.equals("Saturday")||b.equals("Sunday"))
{
System.out.print("yes");
}
else
{
System.out.print("no");
}
}
}

